<% include ../views/index %>
<div>
        <div class="container">
        <ul>
            <% data['articles'].forEach(function(everything){ %>
             <br>
             <li><strong><a href="<%= everything['url'] %>"><%= everything['title'] %></a>
             </strong></li>
             <br>
             <p><%= everything['description'] %></p>
             <br>
             <h6>Published At: <%= everything['publishedAt'], %></h6>    
          <% }) %>
         </ul>
         </div>
  </div>
      